{
  "name": "dbt-language-server",
  "version": "0.0.1",
  "publisher": "Fivetran",
  "preview": true,
  "extensionDependencies": [
    "ms-python.python"
  ],
  "engines": {
    "vscode": "^1.54.0"
  },
  "activationEvents": [
    "workspaceContains:./dbt_project.yml"
  ],
  "main": "./client/out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Configuration",
      "properties": {
        "dbtFivetranExtension.trace.server": {
          "scope": "window",
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "verbose",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    },
    "commands": [
      {
        "command": "editor.showQueryPreview",
        "title": "Show query preview"
      },
      {
        "command": "dbt.compile",
        "title": "dbt compile"
      },
      {
        "command": "editor.afterFunctionCompletion",
        "title": "Do after completion"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "dbt.compile",
          "group": "navigation",
          "when": "dbt-language-server.init && editorLangId =~ /sql$/"
        }
      ],
      "editor/context": [
        {
          "command": "editor.showQueryPreview",
          "group": "navigation@1.31",
          "when": "dbt-language-server.init && editorLangId =~ /sql$/"
        },
        {
          "command": "dbt.compile",
          "group": "navigation@1.31",
          "when": "dbt-language-server.init && editorLangId =~ /sql$/"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "compile:all": "npm run format && npm run compile",
    "watch": "tsc -b -w",
    "format": "prettier --write \"client/src/**/*.ts\" \"server/src/**/*.ts\"",
    "postinstall": "cd client && npm install && cd ../server && npm install && cd ../markdown_parser && npm install && cd ..",
    "lint": "eslint . --ext .ts",
    "test": "mocha \"server/out/test/**/*.spec.js\""
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.5",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "mocha": "^9.1.3",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
